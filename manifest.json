{
  "name": "海拍客HOP助手",
  "version": "1.02",
  "description": "海拍客HOP助手",
  "permissions": [
    "http://*.hipac.cn/"
  ],
  "web_accessible_resources": ["post_script.js", "hook.js"],
  "content_scripts": [{
    "matches": ["http://*.hipac.cn/*"],
    "js": ["post_script.js"],
    "run_at": "document_end"
  }],
  "icons": {
    "16": "images/icon_hipac_16.png",
    "32": "images/icon_hipac_32.png",
    "48": "images/icon_hipac_48.png",
    "128": "images/icon_hipac_128.png"
  },
  "manifest_version": 3
}
